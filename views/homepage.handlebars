{{#each users as |user| }}
<div class="row align-center mb-5">
  <div class="col-md-6">
    <h2>{{{get_emoji}}} {{user.name}}</h2>
  </div>
  <div class="col-md-6">
    <h3><a href="mailto:{{user.email}}">{{user.email}}</a></h3>
  </div>
</div>
{{/each}}

{{!-- Display Posts --}}
{{#each posts as |post|}}
  <div>
   <h2>Latest Posts</h2>
    <h2>{{vibetype}}</h2>
    <h3>{{contentbody}}</h3>
        {{!-- <li>{{title}}</li> --}}
  </div>
  {{/each}}
  








