{{#if posts.length}}
<section>
  <h2>Vibe Posts</h2>
  <ol>
    {{#each posts as |post|}}
      {{#if (eq post.vibetype "yourDesiredPostType")}}
        <li>
          <article class="post">
            <div class="title">
                 <span><a href="/post/{{id}}">{{title}}</a><br>
            </div>
            <div class = "content">
                <p>{{contentbody}}</p>
            </div>
        </article>
          <a href="/dashboard/edit/{{post.id}}" class="edit-link">Edit post</a>
        </li>
      {{/if}}
    {{/each}}
  </ol>
</section>
{{/if}}